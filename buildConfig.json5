{
    copyJobs: [{
        source: "%sewa-saamarth/data",
        target: "%sewa-saamarth/docs/data"
    }, {
        source: "%sewa-saamarth/src/lib",
        target: "%sewa-saamarth/docs/lib"
    }, {
        source: "%sewa-saamarth/src/css",
        target: "%sewa-saamarth/docs/css"
    }, {
        source: "%sewa-saamarth/src/css",
        target: "%sewa-saamarth/docs/css"
    }, {
        source: "%sewa-saamarth/src/html/cart.html",
        target: "%sewa-saamarth/docs/cart/index.html",
        replace: [
            {
            source: "../lib",
            target: "./lib"
        }, {
            source: "../js",
            target: "./js"
        }, {
            source: "../css",
            target: "./css"
        }, {
            source: "renderCart(\"../..\")",
            target: "renderCart(\"..\")"
        }
        ]
    }, {
        source: "%sewa-saamarth/src/js/client/*.js",
        target: "%sewa-saamarth/docs/js/client"
    }, {
        source: "%sewa-saamarth/src/js/shared/*.js",
        target: "%sewa-saamarth/docs/js/shared"
    }, {
        source: "%sewa-saamarth/img/small",
        target: "%sewa-saamarth/docs/img/small"
    }
    ]
}
